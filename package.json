    "docs": "typedoc --entryPoints lib/index.ts",
    "build": "tsc -p tsconfig.json",
    "commit": "gitmoji -c",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
